{
	"name": "@nexxonn-ai/node-registry",
	"version": "0.0.0",
	"private": true,
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"scripts": {
		"build": "tsup",
		"check-types": "tsc --noEmit",
		"format": "biome format --format .",
		"test": "vitest run"
	},
	"exports": {
		"./package.json": "./package.json",
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		}
	},
	"devDependencies": {
		"@nexxonn-ai/tsconfig": "workspace:*",
		"tsup": "catalog:",
		"vitest": "catalog:"
	},
	"dependencies": {
		"@nexxonn-ai/action-registry": "workspace:*",
		"@nexxonn-ai/language-model": "workspace:*",
		"@nexxonn-ai/language-model-registry": "workspace:*",
		"@nexxonn-ai/protocol": "workspace:*",
		"@nexxonn-ai/text-editor-utils": "workspace:*",
		"@nexxonn-ai/trigger-registry": "workspace:*"
	}
}
