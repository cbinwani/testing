{
	"name": "@nexxonn-ai/protocol",
	"version": "0.0.0",
	"private": true,
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"scripts": {
		"build": "tsup",
		"check-types": "tsc --noEmit",
		"clean": "rm -rf .turbo dist node_modules",
		"format": "biome check --write .",
		"test": "vitest run"
	},
	"exports": {
		"./package.json": "./package.json",
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.js",
			"default": "./dist/index.js"
		}
	},
	"devDependencies": {
		"@nexxonn-ai/tsconfig": "workspace:*",
		"@nexxonn-ai/utils": "workspace:*",
		"tsup": "catalog:",
		"vitest": "catalog:"
	},
	"dependencies": {
		"@ai-sdk/provider": "catalog:",
		"@nexxonn-ai/action-registry": "workspace:*",
		"@nexxonn-ai/language-model": "workspace:^",
		"@nexxonn-ai/language-model-registry": "workspace:*",
		"@nexxonn-ai/trigger-registry": "workspace:*",
		"@octokit/webhooks": "catalog:",
		"ai": "catalog:"
	},
	"peerDependencies": {
		"zod": "catalog:"
	}
}
