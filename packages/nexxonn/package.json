		"@nexxonn-ai/vault": "workspace:*",
		"@nexxonn-ai/web-search": "workspace:^",
		"@nexxonn-ai/workspace-utils": "workspace:*",
		"ai": "catalog:",
		"openai": "catalog:",
		"pg": "catalog:"
	},
	"peerDependencies": {
		"zod": "catalog:"
	}
