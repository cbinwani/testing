{
	"name": "@nexxonn-ai/langfuse",
	"version": "0.0.0",
	"private": true,
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"scripts": {
		"build": "tsup",
		"check-types": "tsc --noEmit",
		"format": "biome format --write ."
	},
	"exports": {
		"./package.json": "./package.json",
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		}
	},
	"devDependencies": {
		"@nexxonn-ai/tsconfig": "workspace:*",
		"ai": "catalog:",
		"tsup": "catalog:"
	},
	"dependencies": {
		"@nexxonn-ai/language-model": "workspace:^",
		"@nexxonn-ai/language-model-registry": "workspace:*",
		"@nexxonn-ai/protocol": "workspace:^",
		"@nexxonn-ai/rag": "workspace:^",
		"langfuse": "catalog:"
	}
}
